a=float(input('masukkan nilai a :'))
b=float(input('masukkan nilai b :'))

penjumlahan = a+b
selisih = a-b
perkalian = a*b
sisa = a%b
pembagian = a/b
pangkat =a**b
import math
print ('penjumlahan a ditambah b adalah ',penjumlahan)
print ("selisih a dikurang b adalah", abs (a-b))
print ('perkalian a dikali b', perkalian)
print ('pembagian a dibagi b ' , pembagian)
print ('jumlah sisa pembagian dari hasil a dibagi b adalah', sisa)
print ('hasil a pangkat b', pangkat)
print (' hasil dari log (a) adalah : ' , (math.log10(a)))
