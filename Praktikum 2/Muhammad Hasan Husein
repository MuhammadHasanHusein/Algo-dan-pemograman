print ("hitung luas ruangan")
q=float(input( "masukkan panjang ruangan: "))     
r=float(input ( "masukkan lebar ruangan: "))
debi=input('masukkan satuan (meter/inci): ')

luas= q*r
print ("luas ruangan dengan panjang", q, "dan lebar", r,' adalah :', luas, debi)
